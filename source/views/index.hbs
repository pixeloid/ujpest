---
page_class: page-home
---

<div class='container'>
  <div class='row'>
    <section id='content-primary'>
      <div class='top-news'>
        <div class='top-news__primary'>
          {{> news-item news-item.hero}}
          {{> news-item news-item.split}}
          {{> news-item news-item.split-inverse}}
          {{> news-item news-item.noimage}}
          {{> news-item news-item.split}}
          {{> news-item news-item.split-inverse}}
          {{> news-item news-item.split}}
          {{> news-item news-item.split-inverse}}
        </div>
        <div class='top-news__secondary'>
          {{> news-item news-item}}
          {{> news-item news-item}}
          {{> news-item news-item.noimage}}
          {{> news-item news-item}}
          {{> news-item news-item}}
          {{> news-item news-item.noimage}}

        </div>
      </div>

      <div class="bleed">
        <div class='banner banner--728x90'>
          Hirdetes
        </div>
      </div>

      <div class='news-list'>
        <div class='news-list__col'>
          {{> news-item news-item.noimage}}
          {{> news-item news-item}}
          {{> news-item news-item}}
        </div>
        <div class='news-list__col'>
          {{> news-item news-item}}
          {{> news-item news-item.noimage}}
          {{> news-item news-item}}
        </div>
        <div class='news-list__col news-list__col--last'>
          {{> news-item news-item}}
          {{> news-item news-item}}
          {{> news-item news-item.noimage}}
        </div>
      </div>




    </section>
    <aside id='content-secondary'>
      {{> news-item news-item}}
      {{> news-item news-item.noimage}}
      {{> news-item news-item}}
    </aside>
  </div>
</div>
